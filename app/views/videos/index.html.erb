<p id="notice"><%= notice %></p>
<div class="container">
  <h1 class="text-center">Videos</h1>

  <% @videos.each do |video| %>
    <h3><%= video.title %></h3>
    <p><%= video.summary %></p>
    <div class="embed-responsive embed-responsive-16by9">
      <%= raw video.embed_html %>
    </div>
  <% end %>
</div>
